# BaseSwap Scout Environment Configuration

# API Keys
NEXT_PUBLIC_0X_API_KEY=your_0x_api_key_here
NEXT_PUBLIC_ETHERSCAN_API_KEY=your_etherscan_api_key_here

# RPC Endpoints
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org

# Application Configuration
NEXT_PUBLIC_APP_ENV=development
NEXT_PUBLIC_APP_VERSION=1.0.0

# Feature Flags
NEXT_PUBLIC_ENABLE_ARBITRAGE=true
NEXT_PUBLIC_ENABLE_LIMIT_ORDERS=true
NEXT_PUBLIC_ENABLE_GAS_OPTIMIZATION=true

# Rate Limiting
NEXT_PUBLIC_API_RATE_LIMIT=100
NEXT_PUBLIC_API_RATE_WINDOW=60000

# Cache Configuration
NEXT_PUBLIC_CACHE_TTL=300000
NEXT_PUBLIC_PRICE_CACHE_TTL=60000

# Notification Settings
NEXT_PUBLIC_MAX_NOTIFICATIONS=50
NEXT_PUBLIC_NOTIFICATION_TTL=300000

